---
import { Icon } from 'astro-icon/components';
const { link, description } = Astro.props;
---

<a
  class='rounded-full'
  target='_blank'
  title='Telegram'
  href={`https://t.me/share/url?url=${link}&text=${description}`}
  data-aos='fade-up'
  data-aos-duration='400'
  data-aos-delay='650'
>
  <Icon name='logos-telegram' class='h-10 w-10' />
</a>
